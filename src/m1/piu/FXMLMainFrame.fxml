<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="422.0" prefWidth="725.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="m1.piu.FXMLMainFrameController">
    <children>
      <MenuBar AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="newBtn" mnemonicParsing="false" text="New" />
                  <MenuItem fx:id="openBtn" mnemonicParsing="false" text="Open..." />
                  <MenuItem fx:id="saveBtn" mnemonicParsing="false" text="Save" />
                  <MenuItem fx:id="saveAsBtn" mnemonicParsing="false" text="Save as..." />
                  <MenuItem fx:id="exitBtn" mnemonicParsing="false" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem fx:id="aboutBtn" mnemonicParsing="false" text="About" />
                  <MenuItem fx:id="onlineHelpBtn" mnemonicParsing="false" text="Online Help" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ToolBar layoutY="25.0" prefHeight="40.0" prefWidth="300.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <items>
          <Button fx:id="newContactBtn" mnemonicParsing="false" text="New Contact" />
            <Button fx:id="editContactBtn" layoutX="10.0" layoutY="13.0" mnemonicParsing="false" text="Edit Contact" />
            <Button fx:id="removeContactBtn" layoutX="66.0" layoutY="13.0" mnemonicParsing="false" text="Remove Contact" />
            <Group>
               <children>
                  <TextField fx:id="searchText" layoutX="1.0" />
                  <Button fx:id="searchBtn" layoutX="151.0" mnemonicParsing="false" text="?" />
               </children>
            </Group>
        </items>
      </ToolBar>
      <VBox layoutY="396.0" prefHeight="26.0" prefWidth="725.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Label fx:id="statusBar" text="Selected contact : Toto Titi" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
         </children>
      </VBox>
    </children>
</AnchorPane>
